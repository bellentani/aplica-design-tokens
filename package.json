{
  "name": "aplica-design-tokens",
  "version": "0.0.1",
  "description": "Readme file",
  "main": "index.js",
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.5",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0"
  },
  "scripts": {
    "generate:theme": "ts-node ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "swc src -d dist",
    "start": "NODE_ENV=production node dist/src/index.js",
    "start:dev": "npm run generate:theme",
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/src/index.js",
    "clear:themes": "rm -rf ./temp"
  },
  "dependencies": {
    "@types/color": "^3.0.6",
    "colorjs.io": "^0.5.0",
    "token-transformer": "^0.0.29",
    "ts-node": "^10.9.2"
  },
